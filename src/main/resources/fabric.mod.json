{
	"schemaVersion": 1,
	"id": "yamm-core",
	"version": "${version}",
	"name": "YAMM Core",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"_Katte",
		"PensiveMoth"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT License",
	"icon": "assets/yamm-core/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "net.katte.yamm.YetAnotherMagicMod",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "net.katte.yamm.ExampleModClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "net.katte.yamm.ExampleModDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"yamm-core.mixins.json",
		{
			"config": "yamm-core.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.19.2",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.0"
	},
	"suggests": {
		"another-mod": "*"
	}
}